Core.Landing=Object.create(Core);Core.Landing.init=function(){Core.Landing.setActiveWorkflow();Core.Landing.activeCouponWorkflow();Core.Landing.closeModalUpdatesWorkflow();Core.Landing.fixBackgroundApptLanding()};Core.Landing.Variables={comesFromTires:"comes-from-tires",comesFromCoupons:"comes-from-coupons",comesFromEstimates:"comes-from-estimates",comesFromAppoitment:"comes-from-appoitment",jqCouponButton:$(".get-coupon-workflow"),jqCouponMOdalSearch:$("#coupons-workflow")};Core.Landing.setActiveWorkflow=function(){var n=Core.Variables.ActiveWorkflows,t=Core.Landing.Variables,i=$("#active-workflow");i.hasClass(t.comesFromTires)?Core.Variables.activeWorkflow=n.TIRES_WORKFLOW:i.hasClass(t.comesFromEstimates)?Core.Variables.activeWorkflow=n.ESTIMATES_WORKFLOW:i.hasClass(t.comesFromCoupons)?Core.Variables.activeWorkflow=n.COUPONS_WORKFLOW:i.hasClass(t.comesFromAppoitment)&&(Core.Variables.activeWorkflow=n.BOOK_APPOINTMENT_WORKFLOW)};Core.Landing.activeCouponWorkflow=function(){Core.Landing.Variables.jqCouponButton.click(function(){var n=Core.Variables.ActiveWorkflows;Core.Variables.activeWorkflow=n.COUPONS_WORKFLOW})};Core.Landing.closeModalUpdatesWorkflow=function(){Core.Landing.Variables.jqCouponMOdalSearch.on("closed.zf.reveal",function(){var n=Core.Variables.ActiveWorkflows;Core.Variables.activeWorkflow=n.BOOK_APPOINTMENT_WORKFLOW})};Core.Landing.fixBackgroundApptLanding=function(){var n=$(".black-background-fix");n.length>0&&$("body").css("background-color","black")};Core.Landing.reBrandingVideo=function(){$("#mobile-promo-youtube").on("open.zf.reveal",function(){var n=$(window).height();$("#mobile-promo-youtube iframe").prop("width","100%").prop("height",n+"px")});$(window).resize(function(){var n=$(window).height();$("#mobile-promo-youtube iframe").prop("width","100%").prop("height",n+"px")})};$(window).ready(function(){Core.Landing.init();Core.Landing.reBrandingVideo()})